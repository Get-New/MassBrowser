{
  "comments": false,
  "env": {
    "testing-unit": {
      "presets": ["es2015", "stage-0"],
      "plugins": ["istanbul"]
    },
    "core": {
      "presets": [
        ["es2015", {
          "targets": { "node": 6 },
          "useBuiltIns": true
        }]
      ]
    },
    "main": {
      "presets": ["es2015", "stage-0"]
    },
    "renderer": {
      "presets": [
        ["es2015", { "modules": false }],
        "stage-0"
      ]
    }
  },
  "plugins": ["transform-runtime"]
}
